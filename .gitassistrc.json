{
  "branch": "main",
  "runTests": true,
  "runLint": true,
  "useAI": true,
  "autoCommit": true,
  "autoPush": true,
  "testCommand": "npm run test",
  "lintCommand": "npm run lint",
  "watchIgnore": [
    "*.tmp",
    "build/**",
    "dist/**",
    "node_modules/**",
    ".git/**",
    "*.log",
    ".env*",
    "coverage/**",
    ".nyc_output/**"
  ]
}
